<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nise.jbookproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Actividades.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Actividades.MenuUser"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Actividades.HistorialReservas"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Actividades.ReservarLibro"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Actividades.MenuReserva"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.ReservarComputador"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.ReservarConsola"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.ReservarSala"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.ReservarTelevisor"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.ConsultarReservasActivas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.ReservarSalaDia"
            android:label="@string/title_activity_reservar_sala_dia"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividades.MenuAdmin"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.CrearRecurso"
            android:label="@string/title_activity_crear_recurso"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.CrearUsuario"
            android:label="@string/title_activity_crear_usuario"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.Reglas"
            android:label="@string/title_activity_reglas"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.Noticias"
            android:label="@string/title_activity_noticias"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Actividades.HistorialReservasNav"
            android:label="@string/title_activity_historial_reservas_nav"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Actividades.ConsultarReservasActivasNav"
            android:label="@string/title_activity_consultar_reservas_activas_nav"
            android:screenOrientation="portrait" />
    </application>

</manifest>